{
  "name": "@3yourmind/vue-yodify",
  "version": "0.0.2",
  "description": "A Vue.js component module",
  "author": "Kevin Peters <kpe@3yourmind.com>",
  "main": "dist/index.js",
  "license": "MIT",
  "scripts": {
    "build":
      "cross-env NODE_ENV=production webpack --progress --hide-modules --optimize-minimize",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,json,css,md,vue}": ["prettier --write", "git add"]
  },
  "keywords": ["vue", "component", "notification", "responsive"],
  "files": ["dist/"],
  "dependencies": {
    "vue": "^2.5.13"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.11",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.2.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-module-resolver": "^3.0.0",
    "babel-preset-env": "^1.6.1",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.17.0",
    "eslint-plugin-vue": "4.2.2",
    "file-loader": "^1.1.6",
    "husky": "^0.14.3",
    "jest": "^22.2.2",
    "lint-staged": "^6.1.0",
    "node-sass": "^4.7.2",
    "prettier": "^1.10.2",
    "sass-loader": "^6.0.6",
    "vue-cli": "^2.9.3",
    "vue-jest": "^2.0.1",
    "vue-loader": "^14.1.1",
    "vue-template-compiler": "^2.5.13",
    "vue-template-es2015-compiler": "^1.6.0",
    "vue-webpack-loaders": "^1.0.6",
    "webpack": "^3.10.0"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "vue"],
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "mapCoverage": true
  }
}
